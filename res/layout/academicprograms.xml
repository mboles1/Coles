			<TextView android:id="@+id/textView2" android:layout_width="wrap_content" android:text="Kennesaw State University" android:layout_gravity="center_horizontal" android:textColor="#FFFFFF" android:layout_marginTop="5dp" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content"></TextView>
